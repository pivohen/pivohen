import java.util.Scanner;

public class Main {
    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);
        System.out.print("Введите значение степени n: ");
        int n = scanner.nextInt();

        int value = 1;
        for (int i = 0; i < n; i++) {
            value *= 2;
        }
        
        System.out.println("2 в степени " + n + " равно " + value);
    }
}
